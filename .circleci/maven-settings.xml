<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>aspose.ppt.export</id>
            <username>${env.ENV_CPLACE_ARTIFACTORY_ACTOR}</username>
            <password>${env.ENV_CPLACE_ARTIFACTORY_TOKEN}</password>
        </server>
    </servers>
    <profiles>
        <profile>
            <id>jfrog</id>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <id>cplace-daisydiff</id>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                    <name>cplace-daisydiff</name>
                    <url>https://cplace.jfrog.io/artifactory/cplace-daisydiff/</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
</settings>